    "husky": "8.0.2",
    "jest": "29.3.1",
    "jest-extended": "3.1.0",
    "jest-silent-reporter": "0.5.0",
    "jest-ts-auto-mock": "2.1.0",
    "lint-staged": "13.0.3",
    "madge": "5.0.1",
    "majestic": "1.8.1",
    "node-notifier": "10.0.1",
    "parcel": "2.8.0",
    "prettier": "2.7.1",
    "pug": "3.0.2",
    "rimraf": "3.0.2",
    "semantic-release": "19.0.5",
    "shx": "0.3.4",
    "stryker-cli": "1.0.2",
    "ts-auto-mock": "3.6.2",
    "ts-jest": "29.0.3",
    "ts-node-dev": "2.0.0",
    "tsconfig-paths": "4.1.0",
    "ttypescript": "1.5.13",
    "type-fest": "3.2.0",
    "typescript": "4.9.3",
    "weak-napi": "2.0.2"
  }
}
